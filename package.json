    "@tanstack/react-query": "^5.59.13",
    "firebase": "^10.12.5",
    "jotai": "^2.12.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-firebase-hooks": "^5.1.1",
    "react-icons": "^5.3.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.26.1",
    "swr": "^2.2.5"
